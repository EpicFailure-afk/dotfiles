    // "on-click": "nmcli d wifi list | rofi -dmenu -p 'Select Wi-Fi' | awk '{print $1}' | xargs nmcli d wifi connect"
  },

//* CPU:
  "cpu": {
    "interval": 10, // Update interval in seconds
    "format": "ó°› {usage}%",
    "format-alt": "{icon0}{icon1}{icon2}{icon3}",
    "format-icons": ["â–", "â–‚", "â–ƒ", "â–„", "â–…", "â–†", "â–‡", "â–ˆ"]
  },

//* Memory:
  "memory": {
    "states": {
      "c": 90, // critical
      "h": 60, // high
      "m": 30 // medium
    },
    "interval": 10,
    "format": "ó°¾† {used}GB",
    "format-m": "ó°¾… {used}GB",
    "format-h": "ó°“… {used}GB",
    "format-c": "ï¡ {used}GB",
    "format-alt": "ó°¾† {percentage}%",
    "max-length": 10,
    "tooltip": true,
    "tooltip-format": "ó°¾† {percentage}%\nî‰¦ {used:0.1f}GB/{total:0.1f}GB"
  },

//* Disk 
"disk": {
  "interval": 30,
